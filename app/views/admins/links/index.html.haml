= provide(:title, 'Girls')
= provide(:page_title, 'Girls')
= provide(:page_icon, 'active-page-icon large-gray-icon dress_blk')

- content_for :pageheader do
  %ul.breadcrumb
    %li
      = link_to admins_dashboard_path do
        %i.icon-home
        =t(:dashboard)
      %span.divider /
    %li.active
      %i.icon-official
      =t(:girls)
  %h1#main-heading
    All Links
    %span Links to know

=link_to t(:add), new_admins_link_path, :class => 'btn btn-info pull-right'
.mws-panel.grid_8.mws-collapsible
  .mws-panel-header
    %span
      %i.icon-table
      Links
  .mws-panel-body.no-padding
    %table.mws-datatable-fn.mws-table
      %thead
        %tr
          %th=t(:name)
          %th=t(:address)
          %th=t(:username)
          %th=t(:password)
          %th=t(:active)
          %th=t(:notes)
      %tbody
        - links.each do |l|
          %tr
            %td= l.name
            %td= l.address
            %td= l.username
            %td= l.password
            %td= l.active
            %td= l.notes

!!!
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}/
    %title Pontus - a clever responsive admin template
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}/
    %meta{:content => "Admin Panel Template", :name => "description"}/
    %meta{:content => "Westilian: Kamrujaman Shohel", :name => "author"}/
    / styles
    %link{:href => "/assets/admin/bootstrap.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/styles.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/prettify.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/aristo-ui.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/fullcalendar.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/elfinder.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/jquery.jqplot.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/responsive-tables.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/bootstrap-responsive.css", :rel => "stylesheet"}/
    %link{:href => "/assets/admin/themes.css", :rel => "stylesheet"}/
    /[if IE 7]
      <link rel="stylesheet" type="text/css" href="/asset/admins//ie/ie7.css" />
    /[if IE 8]
      <link rel="stylesheet" type="text/css" href="/asset/admins//ie/ie8.css" />
    /[if IE 9]
      <link rel="stylesheet" type="text/css" href="/asset/admins//ie/ie9.css" />
    / fav and touch icons
    %link{:href => "/assets/admin/ico/favicon.ico", :rel => "shortcut icon"}/
    %link{:href => "/assets/admin/ico/apple-touch-icon-144-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "144x144"}/
    %link{:href => "/assets/admin/ico/apple-touch-icon-114-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "114x114"}/
    %link{:href => "/assets/admin/ico/apple-touch-icon-72-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "72x72"}/
    %link{:href => "/assets/admin/ico/apple-touch-icon-57-precomposed.png", :rel => "apple-touch-icon-precomposed"}/
    / ============ javascript ===========
    %script{:src => "/assets/admin/jquery.js"}
    %script{:src => "/assets/admin/bootstrap.min.js"}
    %script{:src => "/assets/admin/jquery-ui-1.8.16.custom.min.js"}
    %script{:src => "/assets/admin/jquery.data-table.js"}
    %script{:src => "/assets/admin/prettify.js"}
    %script{:src => "/assets/admin/jquery.metadata.js"}
    %script{:src => "/assets/admin/jquery.validate.js"}
    %script{:src => "/assets/admin/stepy.jquery.js"}
    %script{:src => "/assets/admin/inputmask.jquery.js"}
    %script{:src => "/assets/admin/jquery.tagsinput.js"}
    %script{:src => "/assets/admin/uniform.jquery.js"}
    %script{:src => "/assets/admin/fullcalendar.min.js"}
    %script{:src => "/assets/admin/gcal.js"}
    %script{:src => "/assets/admin/elfinder.min.js"}
    %script{:src => "/assets/admin/jquery.notify.js"}
    %script{:src => "/assets/admin/responsive-tables.js"}
    %script{:src => "/assets/admin/jquery.cleditor.js"}
    / RANGE DATE PICKER
    %script{:src => "/assets/admin/jquery.slider.min.js"}
    %script{:src => "/assets/admin/date.js", :type => "text/javascript"}
    %script{:src => "/assets/admin/daterangepicker.js", :type => "text/javascript"}
    %script{:src => "/assets/admin/bootstrap-datepicker.js", :type => "text/javascript"}
    %script{:src => "/assets/admin/bootstrap-colorpicker.js", :type => "text/javascript"}
    %script{:src => "/assets/admin/chosen.jquery.js"}
    %script{:src => "/assets/admin/input-limiter.jquery.js"}
    %script{:src => "/assets/admin/jquery.autosize.js"}

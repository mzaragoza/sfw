    %script{:src => "/assets/admin/chart/jqplot.logAxisRenderer.min.js"}
    %script{:src => "/assets/admin/chart/jqplot.canvasTextRenderer.min.js"}
    %script{:src => "/assets/admin/chart/jqplot.canvasAxisTickRenderer.min.js"}

    %script{:src => "/assets/admin/easing.jquery.js"}
    %script{:src => "/assets/admin/iphone-style-checkboxes.js"}
    %script{:src => "/assets/admin/duallist.jquery.js"}
    %script{:src => "/assets/admin/ui.spinner.min.js"}
    %script{:src => "https://www.google.com/jsapi"}
    %script{:src => "/assets/admin/jquery.sparkline.min.js"}
    / JqPlot CHART
    %script{:src => "/assets/admin/excanvas.min.js"}
    %script{:src => "/assets/admin/jquery.jqplot.min.js"}
    %script{:src => "/assets/admin/chart/jqplot.highlighter.min.js"}

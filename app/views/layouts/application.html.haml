    /[if lt IE 9]
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    %script{:src => "/assets/admin/pontus.script.js"}
    %script{:src => "/assets/admin/respond.min.js"}
    %script{:src => "/assets/admin/ios-orientationchange-fix.js"}
  %body.menu_hover
    .top-bar
      .scroll-top
        %a.tip-top{:href => "#", :title => "Go Top"} Top
      .bar-container
        .container-fluid
          .row-fluid
            .span3
              .logo
                %img{:alt => "Anchor", :height => "68", :src => "/assets/admin/sfw_logo.png", :width => "180"}/
                .btn-group.dropdown-append
                  %a.dropdown-toggle.btn{"data-toggle" => "dropdown", :href => "#", :title => "Recent Updates"}
                    %span.notify-tiptop.tips-green 100
                    %i.gray-icons.list_image
                  .dropdown-menu.msg-dropdown.pull-right
                    %ul.msg-list
                      %li.clearfix
                        %h6.dropdown-h.pull-left Recent Updates
                        %span.pull-right.msg-count
                          %a{:href => "#"} 12 New
                      %li.clearfix
                        %a{:href => "#"}
                          %span.dropdown-thumb
                            %img{:alt => "Avatar", :height => "40", :src => "/assets/admin/order-40x40.png", :width => "40"}/
                          %span
                            %i Order
                            $3,645.8 Dollars order is pending. waiting for approval...
                      %li.clearfix
                        %a{:href => "#"}
                          %span.dropdown-thumb
                            %img{:alt => "Avatar", :height => "40", :src => "/assets/admin/events-40x40.png", :width => "40"}/
                          %span
                            %i Events
                            Total 30 new events is created...
                          %span.notification-meta Last Week
                      %li.clearfix
                        %a{:href => "#"}
                          %span.dropdown-thumb
                            %img{:alt => "Avatar", :height => "40", :src => "/assets/admin/files-40x40.png", :width => "40"}/
                          %span
                            %i Files
                            Total 2,564 files are posted in last week...
                          %span.notification-meta Last Week
                      %li.clearfix
                        %a.btn.btn-block.view-all{:href => "#"} View All
    .navbar.navbar-fixed-top
      .navbar-inner
        .container-fluid
          .nav-collapse
            %ul.nav
              %li
                %a{:href => "index.html"}
                  %i.white-icons.home
                  Home
    .left-bar
      .side-nav
        %ul
          %li.dropdown
            %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
              %span.leftnav-icon.computer_imac_blk
              %span.leftnav-label Home
          %li
            %a{:href => "#"}
              %span.leftnav-icon.folder_blk
              %span.leftnav-label Girls
          %li
            %a{:href => "#"}
              %span.leftnav-icon.chart_8_blk
              %span.leftnav-label XXXXX
          %li
            %a{:href => "#"}
              %span.leftnav-icon.money_blk
              %span.leftnav-label 123123
          %li
            %a{:href => "#"}
              %span.leftnav-icon.help_blk
              %span.leftnav-label 4564564
          %li
            %a{:href => "#"}
              %span.leftnav-icon.cog_3_blk
              %span.leftnav-label ddddd
    .main-wrapper
      .active-page
        %ul
          %li
            %span.active-page-icon.computer_imac_blk
            %span Dashboard
      .container-fluid
        =yield
  %script{:src => "/assets/admin/respond.min.js"}
  %script{:src => "/assets/admin/ios-orientationchange-fix.js"}

  %body.menu_hover
    .top-bar
      .scroll-top
        %a.tip-top{:href => "#", :title => "Go Top"} Top
      .bar-container
        .container-fluid
          .row-fluid
            .span3
              .logo
                %img{:alt => "Anchor", :height => "100", :src => "/assets/admin/sfw_logo.png"}
    .navbar.navbar-fixed-top
      .navbar-inner
        .container-fluid
          .nav-collapse
            %ul.nav
              %li
                %a{:href => "/admins"}
                  %i.white-icons.home
                  Home
            %ul.nav.pull-right
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  %i.white-icons.admin_user_2
                  =current_admin.full_name.titleize
                  %b.caret
                .dropdown-menu.admin-dropdown
                  .clearfix
                    .admin-user-thumb
                      %span.large-gray-icon.admin_user_blk
                    .admin-menu-link
                      %ul
                        %li
                          %a{:href => "#"}
                            %i.gray-icons.user
                            =current_admin.full_name.titleize
                        %li
                          %a{:href => "#"}
                            %i.gray-icons.mail
                            =current_admin.email.downcase
                        -#%li
                          %a{:href => "#"}
                            %i.gray-icons.create_write
                            Edit Profile
                        -#%li
                          %a{:href => "#"}
                            %i.gray-icons.cog_2
                            Settings
                  .admin-action
                    = link_to(destroy_admin_session_path, :method => :delete ) do
                      %button.btn.btn-extend.btn-block{:type => "button"} Logout 
    .left-bar
      .side-nav
        %ul
          %li
            = link_to admins_dashboard_path do
              %span.large-gray-icon.home_blk
              %span.leftnav-label Home
          %li
            = link_to admins_admins_path do
              %span.large-gray-icon.admin_user_blk
              %span.leftnav-label Admins
          %li
            = link_to admins_girls_path do
              %span.large-gray-icon.dress_blk
              %span.leftnav-label Girls
          %li
            = link_to admins_payments_path do
              %span.large-gray-icon.price_tags_blk
              %span.leftnav-label Payments
    .main-wrapper
      .active-page
        %ul
          %li
            -#%span.active-page-icon.computer_imac_blk
            %span{:class => "#{yield(:page_icon)}"}
            %span= yield(:page_title)
      .container-fluid
        = yield :pageheader
        =yield
  = yield :pagefooter
  %script{:src => "/assets/admin/respond.min.js"}
  %script{:src => "/assets/admin/ios-orientationchange-fix.js"}
